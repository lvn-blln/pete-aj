{
 "functions": {
   "api/*.php": {
     "runtime": "vercel-php@0.7.4"
   }
 },
 "routes": [
   { "src": "/css/(.*)", "dest": "/css/$1" },
   { "src": "/images/(.*)", "dest": "/images/$1" },
   { "src": "/fonts/(.*)", "dest": "/fonts/$1" },
   { "src": "/js/(.*)", "dest": "/js/$1" },
   { "src": "/resume/(.*)", "dest": "/resume/$1" },
      { "src": "/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/api/index.php" }
 ]
}