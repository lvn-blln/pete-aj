{
 "functions": {
   "api/*.php": {
     "runtime": "vercel-php@0.7.4"
   }
 },
 "routes": [
   { "src": "/css(.*)", "dest": "/api/css/$1" }
   { "src": "/images(.*)", "dest": "/api/images/$1" }
   { "src": "/js(.*)", "dest": "/api/indejs/$1" }
   { "src": "/resume(.*)", "dest": "/api/resume/$1" }
  { "src": "/(.*)", "dest": "/api/index.php" }
 ]
}